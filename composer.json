{
    "name": "zrashwani/key-pic",
    "description": "psr7 compatible composer package for key pic spam detection webservice",
    "keywords": ["spam", "captcha", "detection"],
    "require": {  
        "php": ">=5.4.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^5.3"        
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.0",
        "symfony/psr-http-message-bridge": "^0.2.0",
        "zendframework/zend-diactoros": "^1.1",
        "symfony/var-dumper": "^2.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zeid Rashwani",
            "homepage": "http://zrashwani.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zrashwani\\KeyPic\\": ["src/KeyPic"] 
        }
    }    
}
